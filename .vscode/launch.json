{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Debug in Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src",
      "urlFilter": "http://localhost:5173/*",
      "sourceMapPathOverrides": {
        "/src/*": "${webRoot}/*"
      }
    }
  ]
}
